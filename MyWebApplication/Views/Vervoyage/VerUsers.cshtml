@model MyWebApplication.Models.ViewModel.VerUsersModel
@{
    ViewBag.Title = "Vervoyage Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Users List</h1>
<div>
    <span class="alert-success">@ViewBag.Message</span>
    <table class="table table-striped table-condensed table-hover">
        <thead>
            <tr>
                <th>Username</th>
                <th>Fullname</th>
                <th>Email</th>
                <th>Birthdate</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model.VerUsers)
            {
                <tr>
                    <td>@Html.DisplayFor(m => i.Username)</td>
                    <td>@Html.DisplayFor(m => i.Fullname)</td>
                    <td>@Html.DisplayFor(m => i.Email)</td>
                    <td>@Html.DisplayFor(m => i.Birthdate)</td>
                </tr>
            }
        </tbody>
    </table>
</div>